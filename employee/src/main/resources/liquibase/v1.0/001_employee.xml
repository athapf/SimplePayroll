<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <changeSet id="1" author="ath">
        <createTable tableName="employee">
            <column name="number" type="text">
                <constraints primaryKey="true"/>
            </column>
            <column name="surename" type="text"/>
            <column name="firstname" type="text"/>
            <column name="street" type="text"/>
            <column name="plz" type="text"/>
            <column name="city" type="text"/>
            <column name="iban" type="text"/>
            <column name="weeklyhours" type="decimal"/>
            <column name="wagerate" type="decimal"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
